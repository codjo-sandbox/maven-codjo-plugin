<project>
    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>net.codjo.maven</groupId>
                <artifactId>codjo-maven-common</artifactId>
                <version>1.6-agi-SNAPSHOT</version>
            </dependency>
        </dependencies>
    </dependencyManagement>
    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <groupId>net.codjo.maven.mojo</groupId>
                    <artifactId>maven-database-plugin</artifactId>
                    <version>1.12-agi-SNAPSHOT</version>
                    <executions>
                        <execution>
                            <goals>
                                <goal>init</goal>
                            </goals>
                        </execution>
                    </executions>
                </plugin>
            </plugins>
        </pluginManagement>
    </build>

</project>